#include<iostream>
using namespace std;
int main()
{
  int n;
  n=5;
  int arr[n]={2,4,1,5,6};
  int i;
  int temp;
 int start=0;
 int end=n-1;
 while(start<end)
 {
   temp=arr[start];
   arr[start]=arr[end];
   arr[end]=temp;
   start++;
   end--;
 }

  for(i=0;i<n;i++)
  {
    cout<<arr[i]<<endl;
  }
}
