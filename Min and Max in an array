#include<iostream>
using namespace std;
int main()
{
  int n;
  n=5;
  int arr[n]={2,4,1,5,6};
  int i;
  int temp;
  int min=0;
  int max=0;
  for(i=0;i<n;i++)
  {
    if(arr[min]<arr[i])
    {
      min=i;
    }
 if(arr[max]>arr[i])
 {
   max=i;
 }
  }
  std::cout << arr[min]<< '\n';
  std::cout <<arr[max]<< '\n';


}
